<Window x:Class="WaiterWPF.Pages.ReturnWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WaiterWPF.Pages"
        mc:Ignorable="d"
        Title="ReturnWindow" Height="400" Width="300"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Barcode"  Margin="5"/>
            <TextBox x:Name="BarcodeTb"  Margin="5" TextChanged="BarcodeTb_TextChanged" PreviewTextInput="BarcodeTb_PreviewTextInput"/>
            <TextBlock Text="Номер заказа"  Margin="5"/>
            <TextBlock Name="NumberBtn" Margin="5"/>
            <TextBlock Text="Имя пользователя"  Margin="5"/>
            <TextBlock Name="UserBtn"  Margin="5"/>
            <TextBlock Text="Продукт"  Margin="5"/>
            <TextBlock Name="NameTb"  Margin="5"/>
            <Button Content="Возврат" x:Name="ReturnBtn" 
                    Click="ReturnBtn_Click"/>
            <Button Content="Выход" x:Name="ExitBtn" 
                    Click="ExitBtn_Click"/>
        </StackPanel>
    </Grid>
</Window>
